{
  "name": "rfidsystem-angular",
  "version": "3.0.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 127.0.0.1 --proxy-config proxy.conf.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "ng build --base-href /rfid/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:cov": "ng test --watch=false --code-coverage",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "sonar": "sonar-scanner",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "start:cov": "http-server -c-1 ./coverage/rfidsystem-angular",
    "start:prod": "http-server ./dist/rfidsystem-angular/ -p 4210",
    "build-and-deploy:prod": "run-s build start:prod",
    "e2e:sample": "start-server-and-test build-and-deploy:prod http://127.0.0.1:4210 cypress:run"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.12",
    "@angular/cdk": "^16.2.11",
    "@angular/common": "^16.2.12",
    "@angular/compiler": "^16.2.12",
    "@angular/core": "^16.2.12",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^16.2.12",
    "@angular/material": "^16.2.11",
    "@angular/platform-browser": "^16.2.12",
    "@angular/platform-browser-dynamic": "^16.2.12",
    "@angular/router": "^16.2.12",
    "@apollo/client": "^3.8.7",
    "@material-design-icons/font": "^0.14.2",
    "@types/crypto-js": "^4.1.1",
    "@types/echarts": "^4.9.16",
    "apollo-angular": "^5.0.2",
    "apollo-link-ws": "^1.0.20",
    "crypto-js": "^4.1.1",
    "echarts": "^5.4.2",
    "graphql": "^16.8.1",
    "japanese-string-utils": "^0.2.4",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "ngx-skeleton-loader": "^6.0.0",
    "ngx-trim-directive": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "roboto-fontface": "^0.10.0",
    "rxjs": "~7.8.0",
    "sonar-scanner": "^3.1.0",
    "sweetalert2": "^11.9.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.9",
    "@angular-eslint/builder": "16.2.0",
    "@angular-eslint/eslint-plugin": "16.2.0",
    "@angular-eslint/eslint-plugin-template": "16.2.0",
    "@angular-eslint/schematics": "16.2.0",
    "@angular-eslint/template-parser": "16.2.0",
    "@angular/cli": "~16.2.9",
    "@angular/compiler-cli": "^16.2.12",
    "@cypress/schematic": "^2.5.0",
    "@types/jasmine": "~4.3.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "cypress": "latest",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "prettier-eslint": "^15.0.1",
    "start-server-and-test": "^2.0.0",
    "typescript": "~4.9.4"
  },
  "overrides": {
    "apollo-angular": {
      "@angular/core": "$@angular/core"
    }
  }
}